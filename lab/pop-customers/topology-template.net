autostart = True
ghostios = True

[localhost:${DYNAMIPS_PORT}]
	workingdir = .
	udp = 5${POP}300
	
	[[7200]]
		image = ../images/${C7200_IMAGE}
		idlepc = ${C7200_IDLEPC}
		ram = ${C7200_RAM}
	
	[[ROUTER POP${POP}-CUST1]]
		model = 7200
		console = 2${POP}010
		aux = 1${POP}011
		slot0 = C7200-IO-2FE
		f0/0 = nio_udp:1${POP}310:127.0.0.1:1${POP}210
	
	[[ROUTER POP${POP}-CUST2]]
		model = 7200
		console = 2${POP}020
		aux = 1${POP}021
		slot0 = C7200-IO-2FE
		f0/0 = nio_udp:1${POP}330:127.0.0.1:1${POP}230
