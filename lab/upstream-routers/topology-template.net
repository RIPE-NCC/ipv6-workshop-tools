autostart = True
ghostios = True

[localhost:7100]
	workingdir = .
	udp = 40200
	
	[[7200]]
		image = ../images/${C7200_IMAGE}
		idlepc = ${C7200_IDLEPC}
		ram = ${C7200_RAM}
	
	[[ROUTER UPSTREAM1]]
		model = 7200
		cnfg = cfg-dual/UPSTREAM1.cfg
		console = 31020
		aux = 31021
		slot0 = C7200-IO-2FE
		slot1 = PA-4E
		slot2 = PA-4E
#		f0/0 = some native interface
		f0/1 = nio_udp:30111:127.0.0.1:30211
		e1/0 = nio_udp:30112:127.0.0.1:30212
		e1/1 = nio_udp:30113:127.0.0.1:30213
