autostart = True
ghostios = True

[localhost:${DYNAMIPS_PORT}]
	workingdir = .
	udp = 40${TRANSIT}00
	
	[[7200]]
		image = ../../images/${C7200_IMAGE}
		idlepc = ${C7200_IDLEPC}
		ram = ${C7200_RAM}
	
	[[ROUTER TRANSIT]]
		model = 7200
		cnfg = cfg-dual/TRANSIT.cfg
		console = 3${TRANSIT}020
		aux = 3${TRANSIT}021
		slot0 = C7200-IO-2FE
		slot1 = PA-4E
		slot2 = PA-4E
#		f0/0 = some native interface
		f0/1 = nio_udp:301${TRANSIT}1:192.168.88.10:302${TRANSIT}1
		e1/0 = nio_udp:301${TRANSIT}2:192.168.88.20:302${TRANSIT}2
		e1/1 = nio_udp:301${TRANSIT}3:192.168.88.10:302${TRANSIT}3
		e1/2 = nio_udp:301${TRANSIT}4:192.168.88.20:302${TRANSIT}4
		e1/3 = nio_udp:301${TRANSIT}5:192.168.88.10:302${TRANSIT}5
		e2/0 = nio_udp:301${TRANSIT}6:192.168.88.20:302${TRANSIT}6
		e2/1 = nio_udp:301${TRANSIT}7:192.168.88.10:302${TRANSIT}7
		e2/2 = nio_udp:301${TRANSIT}8:192.168.88.20:302${TRANSIT}8
		e2/3 = nio_udp:301${TRANSIT}9:192.168.88.10:302${TRANSIT}9
